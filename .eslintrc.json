{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "es6": true,
      "mocha": true,
      "browser": true,
      "jest": true,
      "jquery": true
    },
      "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/destructuring-assignment": "off",
        "react/prop-types": "off",
        "camelcase":"off",
        "jsx-a11y/label-has-for": [ 2, {
          "components": [ "Label" ],
          "some": {
              "every": [ "nesting", "id" ]
          },
          "allowChildren": true
      }],
      "jsx-a11y/label-has-associated-control": "off"
      }
}